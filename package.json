{
  "name": "codetotxt",
  "displayName": "txt_creater",
  "publisher": "Varunpanchal",
  "description": "it converts your code to an txt file , so you can upload your code to chatgpt or any other model rather than uploading your whole codebase one by one",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:file-combiner.openGUI"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "file-combiner.openGUI",
        "title": "codetotxt",
        "icon": "folder.png"
      }
    ]
  },
  "author": "NotVarun",
  "repository": {
    "type": "git",
    "url": "https://github.com/ImNotVarun/CodetoTxt.git"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3",
    "@types/node": "14.x"
  }
}